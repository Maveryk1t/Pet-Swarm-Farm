local tabs = {}
local currentTab = "Main"

local function createTab(name, posX, icon)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0, 95, 1, 0)
    btn.Position = UDim2.new(0, posX, 0, 0)
    btn.BackgroundColor3 = Color3.fromRGB(22, 22, 30)
    btn.Text = icon .. " " .. name
    btn.TextColor3 = Color3.fromRGB(180, 180, 180)
    btn.TextSize = 14
    btn.Font = Enum.Font.Gotham
    btn.Parent = tabFrame
    
    btn.MouseButton1Click:Connect(function()
        currentTab = name
        for _, b in pairs(tabs) do
            b.BackgroundColor3 = Color3.fromRGB(22, 22, 30)
            b.TextColor3 = Color3.fromRGB(180, 180, 180)
        end
        btn.BackgroundColor3 = Color3.fromRGB(65, 105, 225)
        btn.TextColor3 = Color3.fromRGB(255, 255, 255)
        updateContent(name)
    end)
    
    tabs[name] = btn
    return btn
end

createTab("Main", 0, "üè†")
createTab("Eggs", 100, "ü•ö")
createTab("Bosses", 200, "üëë")
createTab("Farms", 300, "‚öôÔ∏è")

local function createLabel(parent, text, yPos, size)
    size = size or 25
    local lbl = Instance.new("TextLabel")
    lbl.Size = UDim2.new(1, -10, 0, size)
    lbl.Position = UDim2.new(0, 5, 0, yPos)
    lbl.BackgroundTransparency = 1
    lbl.Text = text
    lbl.TextColor3 = Color3.fromRGB(220, 220, 220)
    lbl.TextSize = size == 25 and 16 or 14
    lbl.Font = size == 25 and Enum.Font.GothamBold or Enum.Font.Gotham
    lbl.TextXAlignment = Enum.TextXAlignment.Left
    lbl.Parent = parent
    return lbl
end

local function createButton(parent, text, yPos, color, callback)
    color = color or Color3.fromRGB(65, 105, 225)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(1, -20, 0, 40)
    btn.Position = UDim2.new(0, 10, 0, yPos)
    btn.BackgroundColor3 = color
    btn.Text = text
    btn.TextColor3 = Color3.fromRGB(255, 255, 255)
    btn.TextSize = 16
    btn.Font = Enum.Font.GothamBold
    btn.Parent = parent
    btn.MouseButton1Click:Connect(callback)
    return btn
end

local function createToggle(parent, text, yPos, flag, default, callback)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, -10, 0, 35)
    frame.Position = UDim2.new(0, 5, 0, yPos)
    frame.BackgroundTransparency = 1
    frame.Parent = parent
    
    local lbl = Instance.new("TextLabel")
    lbl.Size = UDim2.new(1, -60, 1, 0)
    lbl.BackgroundTransparency = 1
    lbl.Text = text
    lbl.TextColor3 = Color3.fromRGB(220, 220, 220)
    lbl.TextSize = 14
    lbl.Font = Enum.Font.Gotham
    lbl.TextXAlignment = Enum.TextXAlignment.Left
    lbl.Parent = frame
    
    local toggle = Instance.new("TextButton")
    toggle.Size = UDim2.new(0, 50, 0, 25)
    toggle.Position = UDim2.new(1, -55, 0.5, -12.5)
    toggle.BackgroundColor3 = default and Color3.fromRGB(65, 105, 225) or Color3.fromRGB(80, 80, 80)
    toggle.Text = default and "ON" or "OFF"
    toggle.TextColor3 = Color3.fromRGB(255, 255, 255)
    toggle.TextSize = 12
    toggle.Font = Enum.Font.GothamBold
    toggle.Parent = frame
    
    local state = default
    flags[flag] = state
    
    toggle.MouseButton1Click:Connect(function()
        state = not state
        flags[flag] = state
        toggle.BackgroundColor3 = state and Color3.fromRGB(65, 105, 225) or Color3.fromRGB(80, 80, 80)
        toggle.Text = state and "ON" or "OFF"
        callback(state)
    end)
    
    return toggle
end

local function createSlider(parent, text, yPos, min, max, default, suffix, callback)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, -10, 0, 55)
    frame.Position = UDim2.new(0, 5, 0, yPos)
    frame.BackgroundTransparency = 1
    frame.Parent = parent
    
    local lbl = Instance.new("TextLabel")
    lbl.Size = UDim2.new(1, 0, 0, 20)
    lbl.BackgroundTransparency = 1
    lbl.Text = text .. ": " .. default .. suffix
    lbl.TextColor3 = Color3.fromRGB(220, 220, 220)
    lbl.TextSize = 14
    lbl.Font = Enum.Font.Gotham
    lbl.TextXAlignment = Enum.TextXAlignment.Left
    lbl.Parent = frame
    
    local sliderBg = Instance.new("Frame")
    sliderBg.Size = UDim2.new(1, -20, 0, 8)
    sliderBg.Position = UDim2.new(0, 10, 0, 35)
    sliderBg.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
    sliderBg.Parent = frame
    
    local sliderFill = Instance.new("Frame")
    sliderFill.Size = UDim2.new((default - min) / (max - min), 0, 1, 0)
    sliderFill.BackgroundColor3 = Color3.fromRGB(65, 105, 225)
    sliderFill.Parent = sliderBg
    
    local value = default
    
    sliderBg.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            local function update(input)
                local pos = uis:GetMouseLocation().X - sliderBg.AbsolutePosition.X
                local percent = math.clamp(pos / sliderBg.AbsoluteSize.X, 0, 1)
                value = min + (max - min) * percent
                value = math.floor(value * 100) / 100
                sliderFill.Size = UDim2.new(percent, 0, 1, 0)
                lbl.Text = text .. ": " .. value .. suffix
                callback(value)
            end
            update(input)
            local conn
            conn = uis.InputChanged:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
                    update(input)
                end
            end)
            uis.InputEnded:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                    conn:Disconnect()
                end
            end)
        end
    end)
    
    return sliderFill
end

function updateContent(tabName)
    for _, v in ipairs(contentFrame:GetChildren()) do
        if v:IsA("Frame") or v:IsA("TextLabel") or v:IsA("TextButton") then
            v:Destroy()
        end
    end
    
    local yPos = 10
    
    if tabName == "Main" then
        createLabel(contentFrame, "üéÆ CONTROLE", yPos, 30); yPos = yPos + 35
        createButton(contentFrame, "‚ñ∂ INICIAR FARM", yPos, Color3.fromRGB(65, 105, 225), start); yPos = yPos + 50
        createButton(contentFrame, "‚ñ† PARAR FARM", yPos, Color3.fromRGB(220, 50, 50), stop); yPos = yPos + 50
        
        createLabel(contentFrame, "üìä STATUS", yPos, 30); yPos = yPos + 35
        createLabel(contentFrame, "üí∞ Pop-up de coins: MORTO", yPos, 20); yPos = yPos + 25
        createLabel(contentFrame, "‚ö° Cache limpo a cada 45s", yPos, 20); yPos = yPos + 25
        createLabel(contentFrame, "‚úÖ Nests: 0.5s cooldown", yPos, 20); yPos = yPos + 25
        createLabel(contentFrame, "üßπ Console limpo a cada 2s", yPos, 20); yPos = yPos + 25
        
    elseif tabName == "Eggs" then
        createLabel(contentFrame, "ü•ö SELECIONE OS OVOS", yPos, 30); yPos = yPos + 35
        for _, egg in ipairs(eggData) do
            createToggle(contentFrame, egg.name, yPos, "Egg_" .. egg.name, false, function(state)
                if state then
                    eggsSelected[egg.zone] = true
                else
                    eggsSelected[egg.zone] = nil
                end
            end)
            yPos = yPos + 40
        end
        
        yPos = yPos + 10
        createLabel(contentFrame, "‚öôÔ∏è CONFIGURA√á√ïES", yPos, 30); yPos = yPos + 35
        createToggle(contentFrame, "A
